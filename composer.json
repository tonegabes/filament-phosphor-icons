{
  "name": "tonegabes/filament-phosphor-icons",
  "description": "Phosphor Icons Pack ready for Filament 4",
  "type": "library",
  "keywords": [
    "laravel",
    "filament",
    "icons",
    "phosphor",
    "library",
    "iconset"
  ],
  "require": {
    "php": "^8.1",
    "codeat3/blade-phosphor-icons": "^2.3",
    "filament/filament": "^4.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ToneGabes\\Filament\\Icons\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ToneGabes\\Filament\\Icons\\PhosphorIconsServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Tone Gabes",
      "email": "tonegabes@gmail.com",
      "role": "Developer",
      "homepage": "https://tonegabes.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.22"
  }
}
